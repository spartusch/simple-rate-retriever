<configuration>
    <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d %level: %message%n</pattern>
        </encoder>
    </appender>

    <logger name="com.github.spartusch.rateretriever" level="info"/>

    <root level="info">
        <appender-ref ref="stdout"/>
    </root>
</configuration>